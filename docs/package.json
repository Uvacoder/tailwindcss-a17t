{
    "name": "doc",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
      "start": "TAILWIND_MODE=watch NODE_ENV=development npm-run-all prelim:twcss --parallel dev:*",
      "build": "NODE_ENV=production npm-run-all clean prelim:twcss prod:*",
      "prelim:twcss": "./node_modules/tailwindcss/lib/cli.js -i ./assets/css/base.css -o ./static/gen/style.css --jit",
      "dev:twcssw": "./node_modules/tailwindcss/lib/cli.js -i ./assets/css/style.css -o ./static/gen/style.css --jit -w",
      "dev:hugo": "hugo server",
      "prod:twcss": "./node_modules/tailwindcss/lib/cli.js -i ./assets/css/style.css -o ./static/gen/style.css --jit --minify",
      "prod:hugo": "hugo --gc --minify"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "dependencies": {
      "npm-run-all": "^4.1.5",
      "postcss-cli": "^9.1.0",
      "tailwindcss": "^3.0.7"
    }
  }
  